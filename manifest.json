{
    "required_api_version": "^2.0.0",
    "name": "IP Lookup Fork",
    "description": "IP lookup extension for ULauncher",
    "developer_name": "me@munim.net",
    "icon": "images/icon.png",
    "options": {
        "query_debounce": 0.2
    },
    "preferences": [{
            "id": "ipl",
            "type": "keyword",
            "name": "IP Lookup",
            "description": "IP lookup for any IP",
